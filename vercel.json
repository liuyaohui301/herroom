{
  "version": 2,
  "regions": ["hkg1"],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 10
    }
  },
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "X-Edge-Region",
          "value": "hkg1"
        }
      ]
    }
  ]
}